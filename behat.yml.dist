default:
    suites:
        default:
            contexts:
                - App\Tests\Behat\FeatureContext

    extensions:
        FriendsOfBehat\SymfonyExtension:
            bootstrap: tests/bootstrap.php
        Behat\MinkExtension:
            base_url: "%env(BASE_URL)%"
            sessions:
                default:
                    symfony: ~
